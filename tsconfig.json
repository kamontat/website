{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@keystatic/config": ["keystatic.config.ts"],
			"@layouts/*": ["src/layouts/*"],
			"@components/*": ["src/components/*"],
			"@utils/*": ["src/utils/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
