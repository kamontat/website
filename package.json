{
	"name": "website",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"preinstall": "astro telemetry disable && vercel telemetry disable",
		"prettier": "prettier .",
		"eslint": "eslint --ext .js,.cjs,.mjs,.ts,.astro .",
		"check:prettier": "pnpm prettier --check",
		"check:eslint": "pnpm eslint",
		"check:astro": "astro check",
		"check": "pnpm check:prettier && pnpm check:eslint && pnpm check:astro",
		"format:prettier": "pnpm prettier --write",
		"format:eslint": "pnpm eslint --fix",
		"format": "pnpm format:prettier && pnpm format:eslint",
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"deploy:stag": "vercel deploy --yes",
		"deploy:prod": "vercel deploy --yes --prod"
	},
	"dependencies": {
		"@astrojs/check": "0.9.4",
		"@astrojs/markdoc": "0.15.0",
		"@astrojs/react": "4.3.0",
		"@astrojs/sitemap": "3.4.1",
		"@astrojs/svelte": "7.1.0",
		"@astrojs/vercel": "8.2.0",
		"@keystatic/astro": "5.0.6",
		"@keystatic/core": "0.5.47",
		"astro": "5.10.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"svelte": "5.34.7",
		"typescript": "5.8.3"
	},
	"devDependencies": {
		"@types/node": "24.0.3",
		"@types/react": "latest",
		"@types/react-dom": "19.1.6",
		"@typescript-eslint/parser": "8.35.0",
		"eslint": "9.29.0",
		"eslint-plugin-astro": "1.3.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"prettier": "3.6.0",
		"prettier-plugin-astro": "0.14.1",
		"vercel": "44.1.0"
	},
	"packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
	"engines": {
		"node": ">=22 <23"
	}
}
